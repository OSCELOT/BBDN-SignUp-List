{
  "restartable": "rs",
  "verbose": true,
  "ignore": [
    "public",
    "test",
    "node_modules",
    "bower_components",
    ".git"
  ],
  "execMap": {
    "js": "DEBUG=signuplist:* npm start"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
